extend type Mutation {
    createDevice(input: DeviceInput!): Device
    updateDevice(id: UUID!, input: DeviceInput!): Device
    deleteDevice(id: UUID!): Boolean
    markDeviceHistorySynced(id: UUID!): Boolean!

    assignDeviceToUser(deviceId: UUID!, userId: UUID!): Device!
    assignDeviceToOperator(deviceId: UUID!, operatorId: UUID!): Device!
}