type Meter implements Auditable {
    id: UUID!
    name : String
    model: String
    manufacturer: String
    version: String

    powerDispatched: Float
    energyConsumed: Float
    energyProduced: Float

    # Auditable fields
    createdBy: String!
    updatedBy: String
    createdAt: DateTime!
    updatedAt: DateTime
    source: Source!
}