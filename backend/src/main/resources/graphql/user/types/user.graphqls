type User implements Auditable {
    id: UUID!
    email: String!
    firstname: String!
    lastname: String!
    password: String!
    role: Role!
    enabled: Boolean!

    usedDevices: [Device!]!
    operatedDevices: [Device!]!

    createdBy: String!
    updatedBy: String
    createdAt: DateTime!
    updatedAt: DateTime
    source: Source!
}