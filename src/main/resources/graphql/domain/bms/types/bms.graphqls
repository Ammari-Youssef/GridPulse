type Bms implements Auditable {
    id: UUID!
    name: String
    model: String
    manufacturer: String
    version: String

    soh: BatteryHealthStatus
    soc: Float
    batteryChemistry: BatteryChemistry
    cycles: Int
    temperature: Float
    voltage: Float

    createdBy: String!
    updatedBy: String
    createdAt: DateTime!
    updatedAt: DateTime
    source: Source!
}