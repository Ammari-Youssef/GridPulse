type Message implements Auditable {
    id: ID!
    device: Device!
    cloudMessageId: String
    messageText: String!
    messageType: MessageType!
    status: MessageStatus!
    severity: Severity!
    format: MessageFormat!
    priority: MessagePriority!
    explanation: String
    sentAt: String!
    receivedAt: String!

    # Auditable fields
    createdBy: String!
    updatedBy: String
    createdAt: String!
    updatedAt: String
    source: Source!
}