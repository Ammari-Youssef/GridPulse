type UserHistory implements Auditable & AuditableHistory {
    id: ID!
    originalId: ID!
    firstname: String
    lastname: String
    email: String
    role: String
    enabled: Boolean

    createdBy: String!
    updatedBy: String
    createdAt: String!
    updatedAt: String
    source: Source!

    createdRecord: Boolean!
    updatedRecord: Boolean!
    deletedRecord: Boolean!
    synced: Boolean!
}